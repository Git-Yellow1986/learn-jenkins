pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
    }
    stages {
        stage('Test') {
            steps {
                echo 'Welcome to India'
            }
        }
    }
    stages {
        stage('Deploy') {
            steps {
                echo 'Pipeline'
            }
        }
    }
}